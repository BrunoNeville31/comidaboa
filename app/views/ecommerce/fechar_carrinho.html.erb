<div class="col-lg-9">
   <h1>Carrinho</h1>
  <div class="row"> 
   
  <% @produto.each do |produto| %>
    <div class="col-lg-4 col-md-6 mb-4">
      <div class="card h-100">
        <a href="#"><img style="width: 250px; height: 150px;" class="card-img-top" src="<%= produto.imagem %>" alt="" ></a>
        <div class="card-body">
          <h4 class="card-title">
            <a href="/produto/<%= produto.id %>"><%= produto.nome %></a>
          </h4>
          <h5>$24.99</h5>
          <p class="card-text"><%= produto.descricao %></p>
        </div>        
      </div>
    </div>
    <% end %>       

  </div>
  <!-- /.row --> 
  <div class="row">
  <div>
    <h1>Boleto</h1>
   <a href="#"> <img src="data:image/jpeg;base64,"></a>
  </div>

  <div>
    <h1>Cartão</h1>
   <a href="#"> <img src="data:image/png;base64,"></a>
  </div>
</div>
    <div class="row">
      <a href="/carrinho/fechar" class="btn btn-success" style="width: 100%;margin: 20px; font-size: 40px ">Concluir Compra</a>
    </div> 
</div>


